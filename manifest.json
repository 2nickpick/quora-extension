{
    "manifest_version": 2,

    "name": "Quora Extension by 2NickPick (forked by ShifarShifz)",
    "description": "Utilities for using Quora.com",
    "version": "0.1",

    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["https://*.quora.com/*"],
            "js": ["lib/jquery.min.js", "main.js"],
            "runat": "document_start"
        }
    ]
}